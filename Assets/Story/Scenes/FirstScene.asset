    sound: {fileID: 8300000, guid: e772c0e4b77f073418cfa2208a6271b5, type: 3}
  - text: "\u041D\u043E \u0435\u0449\u0451 \u0435\u0441\u0442\u044C \u0441\u043F\u043E\u0441\u043E\u0431
      \u043F\u0435\u0440\u0435\u043B\u043E\u043C\u0438\u0442\u044C \u0438\u0441\u0445\u043E\u0434
      \u044D\u0442\u043E\u0439 \u0432\u043E\u0439\u043D\u044B. \u0414\u043B\u044F
      \u044D\u0442\u043E\u0433\u043E \u044F \u0441\u043E\u0431\u0440\u0430\u043B
      \u0437\u0434\u0435\u0441\u044C \u044E\u043D\u044B\u0445 \u0414\u0436\u0443\u043D-\u0441\u0430\u043D\u043E\u0432,
      \u0438 \u0442\u044B, Ivan \u0414\u0436\u0443\u043D-\u0441\u0430\u043D, \u0432
      \u0438\u0445 \u0447\u0438\u0441\u043B\u0435."
    speaker: {fileID: 11400000, guid: 3d6197ceaa0daca48a5724f78f5c0b9a, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 3d6197ceaa0daca48a5724f78f5c0b9a, type: 2}
