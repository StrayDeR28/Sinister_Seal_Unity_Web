    music: {fileID: 8300000, guid: a50f20b9f679d9749a6e5b3afe46c53d, type: 3}
    sound: {fileID: 8300000, guid: 29d124bc991565d4c8e57ae819ae9e59, type: 3}
  - text: "\u041E\u0434\u043D\u043E\u0439 \u0442\u0432\u043E\u0435\u0439 \u043C\u0430\u0433\u0438\u0438
      \u0440\u0443\u043D \u0431\u0443\u0434\u0435\u0442 \u043C\u0430\u043B\u043E.
      \u041D\u0430\u043C \u043D\u0443\u0436\u043D\u043E \u0441\u043E\u0432\u0435\u0440\u0448\u0435\u043D\u0441\u0442\u0432\u043E\u0432\u0430\u0442\u044C
      \u0432\u0441\u0435 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u044B\u0435 \u043E\u0431\u043B\u0430\u0441\u0442\u0438
      \u043C\u0430\u0433\u0438\u0438."
    speaker: {fileID: 11400000, guid: ef7e04e6da7f11c40a454a0cbba37f39, type: 2}
    actions:
